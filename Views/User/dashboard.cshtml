@{
    ViewData["Title"] = "User Dashboard";
}
@model IEnumerable<Dashboard.Models.User>

<div class="row">
   <h1 class="col-md-3">All Users</h1>
</div>
<div class="row">
   <table class="table table-hover">
      <thead>
         <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Created At</th>
            <th>User Level</th>
         </tr>
      </thead>
   </table>
</div>